local library = loadstring(game:HttpGet(('https://raw.githubusercontent.com/AikaV3rm/UiLib/master/Lib.lua')))()
 
local w = library:CreateWindow("Korke.ml") -- Creates the window
 
local b = w:CreateFolder("Arsenal") -- Creates the folder(U will put here your buttons,etc)

local Dust = w:CreateFolder("Dust") -- Creates the folder(U will put here your buttons,etc)

local a = w:CreateFolder("Assassin") -- Creates the folder(U will put here your buttons,etc)

local s = w:CreateFolder("Superpowertraining") -- Creates the folder(U will put here your buttons,etc)
 
b:Label("Korke.ml",{
    TextSize = 25; -- Self Explaining
    TextColor = Color3.fromRGB(255,255,255); -- Self Explaining
    BgColor = Color3.fromRGB(69,69,69); -- Self Explaining
    
}) 
 
b:Button("silent aim",function()
    loadstring(game:HttpGet("https://pastebin.com/raw/DUeHdyvk", true))()
end)
 
a:Button("Auto collect candy",function()
workspace.Eggs.ChildAdded:connect(function(v)
v.CFrame = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame
end)
end)

b:Button("silent aim",function()
    loadstring(game:HttpGet("https://pastebin.com/raw/DUeHdyvk", true))()
end)
 
s:Button("body",function()
    while true do
wait()
local A_1 =
{
[1] = "+BT1"
}
local Event = game.ReplicatedStorage.RemoteEvent
Event:FireServer(A_1)
wait()
local A_1 =
{
[1] = "+BT2"
}
local Event = game.ReplicatedStorage.RemoteEvent
Event:FireServer(A_1)
wait()
local A_1 =
{
[1] = "+BT3"
}
local Event = game.ReplicatedStorage.RemoteEvent
Event:FireServer(A_1)
end
end)

a:Button("SilentAim",function()
getgenv().SelectedPart = "Head"
getgenv().VisibiltyCheck = true
getgenv().TargetESP = false
getgenv().FOV = 35000000
getgenv().CircleVisibility = true
getgenv().Distance = 500000
loadstring(game:HttpGet("https://raw.githubusercontent.com/yesok3877/Celestial-Silent-Aim/master/Auto-Input", true))()
end)

b:Button("Esp",function()
     local gplrs = game.Players:GetPlayers()
local plr = game.Players.LocalPlayer
local TorsoEsp = Instance.new("Part")
TorsoEsp.Anchored = true
TorsoEsp.BottomSurface = Enum.SurfaceType.Smooth
TorsoEsp.CFrame = CFrame.new(-153.01, 3, -34.152)
TorsoEsp.Name = "TorsoEsp"
TorsoEsp.Reflectance = 1
TorsoEsp.Size = Vector3.new(0.05, 1.66, 0.05)
TorsoEsp.TopSurface = Enum.SurfaceType.Smooth
TorsoEsp.Transparency = 1
TorsoEsp.Parent = game:GetService("Workspace")
TorsoEsp.CanCollide = false

local BoxHandleAdornment = Instance.new("BoxHandleAdornment")
BoxHandleAdornment.Adornee = nil
BoxHandleAdornment.AlwaysOnTop = true
BoxHandleAdornment.Color3 = Color3.fromRGB(170, 85, 255)
BoxHandleAdornment.Size = Vector3.new(2, 2, 1)
BoxHandleAdornment.Transparency = 0.5
BoxHandleAdornment.ZIndex = 2
BoxHandleAdornment.Parent = TorsoEsp



game.Players.PlayerAdded:Connect(function(plr)
    TorsoEsp:Clone()
    TorsoEsp.Parent = game.Workspace:WaitForChild(plr.Name)
    TorsoEsp.BoxHandleAdornment.Adornee = game.Workspace:WaitForChild(plr.Name).HumanoidRootPart
end)


for i, v in pairs(gplrs) do
    local BoxHandleAdornment = Instance.new("BoxHandleAdornment")
BoxHandleAdornment.Adornee = v.Character.HumanoidRootPart
BoxHandleAdornment.AlwaysOnTop = true
BoxHandleAdornment.Color3 = Color3.fromRGB(170, 85, 255)
BoxHandleAdornment.Size = Vector3.new(2, 2, 1)
BoxHandleAdornment.Transparency = 0.5
BoxHandleAdornment.ZIndex = 2
BoxHandleAdornment.Parent = v.Character
end

wait()
local plr = game.Players.LocalPlayer
game.Workspace:WaitForChild(plr.Name).BoxHandleAdornment:Destroy()
end)

Dust:Button("Esp",function()
    local gplrs = game.Players:GetPlayers()
